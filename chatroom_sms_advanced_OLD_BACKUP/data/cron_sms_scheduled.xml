<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Cron Job: Send Scheduled SMS -->

        <record id="ir_cron_sms_scheduled_send" model="ir.cron">
            <field name="name">SMS - Enviar SMS Agendados</field>
            <field name="model_id" ref="model_chatroom_sms_scheduled"/>
            <field name="state">code</field>
            <field name="code">model.cron_send_scheduled_sms()</field>
            <field name="interval_number">5</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="active" eval="True"/>
            <field name="priority">5</field>
            <field name="doall" eval="False"/>
        </record>

    </data>
</odoo>
